<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:custom="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/rl_log_out_dialog"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/black50"
    android:gravity="center"
    android:paddingStart="@dimen/common_dialog_body_padding_left"
    android:paddingLeft="@dimen/common_dialog_body_padding_left"
    android:paddingEnd="@dimen/common_dialog_body_padding_right"
    android:paddingRight="@dimen/common_dialog_body_padding_right"
    tools:ignore="MissingPrefix">
    
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/white"
        android:orientation="vertical">
        
        <TextView
            android:id="@+id/tv_title_logout_dialog"
            style="@style/TextStyleRobotoMedium"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="21dp"
            android:layout_marginRight="24dp"
            android:gravity="center"
            android:text="提示"
            android:textColor="@color/black"
            android:textSize="20sp" />
        
        <TextView
            style="@style/TextStyleRobotoMedium"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="21dp"
            android:layout_marginRight="24dp"
            android:layout_marginBottom="40dp"
            android:gravity="center"
            android:text="写入权限失败"
            android:textColor="@color/black"
            android:textSize="16sp" />
    
    </LinearLayout>

</RelativeLayout>